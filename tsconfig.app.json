/* App specific TypeScript configuration file. See https://www.typescriptlang.org/tsconfig/ */

{
	"extends": "@vue/tsconfig/tsconfig.dom.json",
	"include": ["src/**/*", "src/**/*.vue"],
	"exclude": ["src/**/__tests__/*"],
	"compilerOptions": {
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
		"paths": {
			"@/*": ["./src/*"]
		},
		"composite": true
	}
}
